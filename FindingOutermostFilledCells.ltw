                <Value xsi:type="xsd:string">using System;
using System.Collections.Generic;
using System.Text;
using LTools.Scripting.Model;
using LTools.Network.Model;

public class PrimoScript
{
    public static LTools.Scripting.CSharp.ScriptDebugger __debug;

    public void main(LTools.Common.Model.WorkflowData wf)
    {
        String str_linkExcelFile = wf.GetArgument("in_linkExcelFile").ToString();
        String str_nameSheetRangeCopied = wf.GetArgument("in_nameSheetRangeCopied").ToString();

        Workbook wb_mainExcelFileReadingRange = new Workbook();//Объявление основного Excel файла с которого будет считываться используемый диапазон
        wb_mainExcelFileReadingRange.LoadDocument(str_linkExcelFile);//Подгрузка основного Excel файла

        Worksheet ws_rangeReadingSheet = wb_mainExcelFileReadingRange.Worksheets[str_nameSheetRangeCopied];//Получение листа для считывания диапазона
        CellRange cr_changedRange = ws_rangeReadingSheet.GetUsedRange();//Получение измененного диапазона
        wf.SetArgument("out_changedRangeExcel", cr_changedRange.GetReferenceA1());//Передача диапазона в основной процесс, в аргумент out_changedRangeExcel
    }
}</Value>
